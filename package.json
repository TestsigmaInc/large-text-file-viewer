{
    "name": "react-log-file-viewer",
    "version": "1.0.7",
    "description": "A React component for viewing large text files.",
    "main": "dist/index.js",
    "module": "src/index.js",
    "exports": {
      ".": {
        "import": "./src/index.js",
        "require": "./dist/index.js"
      }
    },
    "scripts": {
      "build": "babel src --out-dir dist --copy-files",
      "prepublish": "npm run build"
    },
