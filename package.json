      "react",
      "component",
      "text file",
      "viewer",
      "large file"
    ],
    "author": "Vinodh Kumar G",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/TestsigmaInc/react-log-file-viewer"
    },
    "peerDependencies": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "devDependencies": {
      "@babel/cli": "^7.22.5",
      "@babel/preset-env": "^7.22.5",
      "@babel/preset-react": "^7.22.5",
      "babel-loader": "^9.1.2",
      "webpack": "^5.87.0",
      "webpack-cli": "^5.1.4",
      "workerize-loader": "^2.0.2"
    }
}
  